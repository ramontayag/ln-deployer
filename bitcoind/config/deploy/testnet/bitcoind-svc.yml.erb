---
apiVersion: v1
kind: Service
metadata:
  name: bitcoind
  <<: <%= partial "namespace" %>
  labels:
    app: bitcoind
spec:
  ports:
    - port: 18332
      targetPort: rpc-port
      name: rpc-port
    - port: 18333
      targetPort: listen-port
      name: listen-port
    - name: zmqblock-port
      targetPort: zmqblock-port
      port: 29000
    - name: zmqtx-port
      targetPort: zmqtx-port
      port: 29001
  selector:
    app: bitcoind
